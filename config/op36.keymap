        };

        combo_yo_en {
            bindings = <&ru RU_CYRILLIC_IO>;
            key-positions = <18 19>;
            layers = <0>;
        };

        parentheses {
            bindings = <&parentheses>;
            key-positions = <11 13>;
            timeout-ms = <30>;
        };

        combo_braces_ru {
            bindings = <&braces_ru>;
            key-positions = <16 18>;
            timeout-ms = <25>;
