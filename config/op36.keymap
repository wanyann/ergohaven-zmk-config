        ru_com_ques: ru_com_ques {
            compatible = "zmk,behavior-mod-morph";
            label = "RU_COM_QUES";
            bindings = <&ru_cw_off RU_COMMA>, <&ru_cw_off RU_QUESTION>;

            #binding-cells = <0>;
            mods = <(MOD_LSFT)>;
        };

        o_morph: o_morph {
            compatible = "zmk,behavior-mod-morph";
            label = "O_MORPH";
            bindings = <&kp O>, <&kp E>;

            #binding-cells = <0>;
            mods = <(MOD_LCTL)>;
            keep-mods = <(MOD_LCTL)>;
        };

        s_morph: s_morph {
            compatible = "zmk,behavior-mod-morph";
            label = "S_MORPH";
            bindings = <&kp S>, <&kp L>;

