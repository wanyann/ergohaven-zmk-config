            tapping-term-ms = <200>;
            flavor = "tap-preferred";
        };

        mkp_hold_msoff: mkp_hold_msoff {
            compatible = "zmk,behavior-hold-tap";
            label = "MKP_HOLD_MSOFF";
            bindings = <&kp_msoff>, <&mkp>;

            #binding-cells = <2>;
            tapping-term-ms = <200>;
            flavor = "tap-preferred";
