            layers = <0 1 10 13>;
        };

        combo_ru_semi {
            bindings = <&kp RU_SEMI>;
            key-positions = <3 4>;
            layers = <2 3 11 14>;
        };

        combo_ru_amp {
            bindings = <&en AMPS>;
            key-positions = <23 24>;
            layers = <2 3 11 14>;
        };

        combo_en_colon {
            bindings = <&kp COLON>;
